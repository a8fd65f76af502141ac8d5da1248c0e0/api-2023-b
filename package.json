{
  "name": "app",
  "version": "0.0.1",
  "description": "App",
  "main": "index.js",
  "scripts": {
    "docker:up:dev": "docker compose up --build -d db",
    "env:dump": "echo > .env && rm .env && cat env.example > .env",
    "git:push": "npm run lint && git push",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{cli,src,test}/**/*.ts\" && prettier --loglevel=error \"{cli,src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{cli,src,test}/**/*.ts\" --fix && prettier --loglevel=error --write \"{cli,src,test}/**/*.ts\"",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watchAll --coverage",
    "test:cov": "jest --coverage --runInBand --coverageDirectory=./coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.3.12",
    "@nestjs/platform-express": "9.3.12",
    "@nestjs/schedule": "2.2.0",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/typeorm": "9.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "nest-commander": "^3.12.5",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "typeorm": "0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@redocly/cli": "^1.0.0-beta.125",
    "@types/amqplib": "^0.10.1",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "jest-junit": "^15.0.0",
    "pg-mem": "^2.6.12",
    "prettier": "2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typedoc": "^0.23.28",
    "typedoc-theme-hierarchy": "^3.1.0",
    "typemoq": "^2.1.0",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0"
  }
}
